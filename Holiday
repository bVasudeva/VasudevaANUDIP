class Holiday
{
	String holidayname;
	int holiday;
	String  holidaymonth;
public Holiday()
{
  		holidayname="Sankranthri";
		holiday= 8;
		holidaymonth="january";
}
	public Holiday(String name,int day,String month)
	{
		holidayname=name;
		holiday= day;
		holidaymonth=month;
	}
	public static void main(String args[])
	{
		Holiday H= new Holiday("Dasara",20,"june");
		System.out.println(H.holidayname+" ");
		System.out.println(H.holiday+" ");
		System.out.println(H.holidaymonth+"  ");
	}
}




